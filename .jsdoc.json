{
  "tags": {
    "allowUnknownTags": false,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["cypress/integration/common", "README.md"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": ["plugins/markdown", "jsdoc-cypress-cucumber-plugin"],
  "templates": {
    "referenceTitle": "Mobomo Cypress QA",
    "collapse": true,
    "resources": {
      "google": "https://www.google.com/"
    }
  },
  "opts": {
    "destination": "./docs/",
    "encoding": "utf8",
    "private": false,
    "recurse": true,
    "template": "./src/jsdoc-template"
  }
}
