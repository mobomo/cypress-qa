{
  "name": "cypress-cucumber-example",
  "version": "1.0.0",
  "description": "An example for Cypress and Cucumber usage",
  "main": "index.js",
  "scripts": {
    "cypress:open": "cypress open",
    "test": "cypress run --env TAGS='@e2e-test' --spec 'cypress/integration/**/*.feature'",
    "test:debug": "cypress run --headed --env TAGS='@e2e-test' --spec 'cypress/integration/**/*.feature' --no-exit",
    "test:tag": "cypress run --headed --env TAGS='@content' --spec 'cypress/integration/**/*.feature' --no-exit"
  },
  "author": "",
  "license": "Private",
  "dependencies": {
    "aws-sdk": "^2.951.0",
    "axios": "^0.21.0",
    "cypress": "^7.7.0",
    "cypress-cucumber-preprocessor": "^4.1.4",
    "fs-extra": "^10.0.0",
    "glob": "^7.1.7"
  },
  "devDependencies": {
    "eslint": "^7.30.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.0.1"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint",
      "git add"
    ]
  }
}
